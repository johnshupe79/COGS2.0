<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.nick.COGS">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="COGS"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.nick.COGS.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.nick.COGS.RegisterActivity" />
        <activity android:name="com.example.nick.COGS.UserActivity" />
        <activity android:name="com.example.nick.COGS.SplashPage" />
        <activity android:name="com.example.nick.COGS.TopicActivity" />
        <activity android:name="com.example.nick.COGS.Notications" />
        <activity android:name="com.example.nick.COGS.StepsActivity"></activity>
    </application>

</manifest>
